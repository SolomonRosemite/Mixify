  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "mixify, plan 1",
      "cargo": {
        "args": ["build", "--bin=mixify", "--package=mixify"],
        "filter": {
          "name": "mixify",
          "kind": "bin"
        }
      },
      "args": ["plan", "1"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "mixify, apply 2",
      "cargo": {
        "args": ["build", "--bin=mixify", "--package=mixify"],
        "filter": {
          "name": "mixify",
          "kind": "bin"
        }
      },
      "args": ["apply", "2"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
